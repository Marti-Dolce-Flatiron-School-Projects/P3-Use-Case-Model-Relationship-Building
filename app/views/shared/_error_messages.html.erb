<% if flash[:messages] %>
  <div id="error_explanation">
    <div class="alert alert-danger">
      The form contains <%= pluralize(flash.messages.count, "error") %>.
    </div>
    <ul>
      <% object.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
    </ul>
  </div>
<% end %>
